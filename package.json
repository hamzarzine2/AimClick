{
  "name": "js-frontend-boilerplate",
  "version": "1.0.0",
  "description": "web2 frontend boilerplate to facilitate deployment to GitHub pages & authentication",
  "scripts": {
    "deploy": "npm run build && gh-pages -d dist",
    "dev": "webpack --mode development && cp dist/index.html dist/404.html",
    "build": "webpack --mode production && cp dist/index.html dist/404.html",
    "start": "webpack serve --mode development",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "code:fix": "prettier --write . && eslint --fix .",
    "commit": "cz"
  },
  "author": "e-baron",
  "license": "CC-BY-SA-4.0",
  "homepage": ".",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "1.19.1",
    "prettier-airbnb-config": "^1.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@mojs/core": "^1.3.0",
    "@popperjs/core": "^2.9.2",
    "animejs": "^3.2.1",
    "bootstrap": "^5.0.1",
    "gh-pages": "^4.0.0",
    "gsap": "^3.11.3",
    "jquery": "^3.6.1",
    "jsbarcode": "^3.11.5",
    "prettier": "^2.8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
